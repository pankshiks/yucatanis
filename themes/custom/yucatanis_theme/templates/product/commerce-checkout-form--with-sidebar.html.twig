{#
/**
 * @file
 * Two column template for the checkout form.
 *
 * Available variables:
 * - form: The form.
 *
 * @ingroup themeable
 */
#}
<section class="">
  <div class="container">
    <nav class="breadcrumb font16">
    <a href="/form/personal-details/?order_id={{orderId}}">Go Back</a>
    <span class="separator"> > </span>
      <a href="#">1. Personal </a>
      <span class="separator"> > </span>
      <span class="current-page">2. Payment</span>

    </nav>
  </div>
</section>
<section class="payments-block">
  <div class="container">
    <div class="layout-checkout-form clearfix">
        <div class="layout-region layout-region-checkout-main">
            <div class="personal-details-wrapper">
               <div class="personal-details-block payments-block">
              
                  {{ form|without('sidebar', 'actions') }}

                  <div class="layout-region layout-region-checkout-footer">
                    {{ form.actions }}
                  </div>
                  </div>
              </div>
          </div>

      <div class="layout-region layout-region-checkout-secondary">
        <div class="order-summary-block" style="padding-bottom: 10px;">
          <h5>{% trans %} Order summary {% endtrans %}</h5>
          {{ form.sidebar }}
          </div>
      </div>
      </div>
  </div>
</section>