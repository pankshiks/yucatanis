<section class="bg-tertiary ptb-132 img-desc-block relevant-block">
  <div class="container">
    <div class="row">
      <div class="col-12">
        <div class="blocks-wrapper">
          <div class="content-block">
             {{ paragraph.field_avheading.value|raw}}
             {{paragraph.field_avdescription.value|raw}}
          </div>
          <div class="image-block">
            <div class="four-block-img">
              {% for image in paragraph.field_avimg %}
                <img src="{{ file_url(image.entity.field_media_image.entity.fileuri) }}" alt="{{image.entity.field_media_image.alt}}">

              {% endfor %}
            </div>
          </div>
        </div>
      </div>
    </div>
</section>

