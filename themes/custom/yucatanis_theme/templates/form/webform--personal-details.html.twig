            <div class="select-items floating-input">
              {# <input type="email" id="floating-input" value="" required="required">
              <label class="" for="Email">Favorite Email*</label> #}
              {{ element.elements.email }}
              <label class="" for="Email"> {{ element.elements.email['#title'] }}</label>
            </div>
            <div class="select-items floating-input">
              {{ element.elements.phone_number }}
              <label for="phone" class="">{{ element.elements.phone_number['#title'] }}</label>
            </div>
            <div class="select-items floating-input">
        
                {{ element.elements.country }}
              <label class="" for="Country of Origin">{{ element.elements.country['#title'] }}</label>
            </div>

            <div class="select-items floating-input">
              
              {{ element.elements.hotel_information }}
                <label class="" for="text">{{ element.elements.hotel_s_name_for_pick_up['#title']  }}</label>
                {# <label class="" for=""> {{ element.elements.hotel_information['#title'] }}</label>  #}
            </div>
            <div class="select-items floating-input">
        
                {# {{ element.elements.hotel_s_name_for_pick_up }}
                <label class="" for=""> if_your_beach_hotel_is_not_listed</label>  #}
                {{ element.elements.if_your_beach_hotel_is_not_listed }}
            </div>
            <div class="select-items floating-input">
           
                 {{ element.elements.room_number }}
                 <label class="" for="text">{{ element.elements.room_number['#title'] }}
                <span class="footnote">They ask the driver at your hotelâ€™s front
                  gate at the time we pick you up (for safety).</span>
              </label>
            </div>
            <div class="select-items floating-input">
              <label class="" for="text">EXACT PICK UP TIME</label>
                {{ element.elements.output }}
            </div>
          
         
            <div class="select-items floating-input">
            
                    {{ element.elements.note }}
                    <label class="" for="text">{{ element.elements.note['#title'] }}
